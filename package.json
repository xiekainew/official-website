{
  "name": "qiankun-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/vue": "^10.9.0",
    "@vueuse/core": "^13.9.0",
    "core-js": "^3.8.3",
    "d3": "^7.9.0",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.2.1",
    "pinia": "^3.0.3",
    "sass": "^1.92.0",
    "three": "^0.180.0",
    "vue": "^3.2.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
